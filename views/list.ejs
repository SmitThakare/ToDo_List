<%- include("header") -%>
    <div class="box" id="heading">
        <h1>
            <%=ListTitle%>
        </h1>
    </div>
    <div class="box">
        <h2>The List</h2>
        <% for (var i=0; i < newListItem.length; i++) { %>
            <div class="item">
                <input type="checkbox">

                <p>
                    <%= newListItem[i] %>
                </p>



            </div>
            <% } %>
                <form class="" action="/" method="post">

                    <input type="text" name="newItem" placeholder="New Item">
                    <button type="submit" name="list" value=<%=ListTitle%> >+</button>
                </form>

    </div>
    <%- include("footer") -%>